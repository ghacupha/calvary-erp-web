    "@typescript-eslint/parser": "5.36.1",
    "autoprefixer": "10.4.8",
    "browser-sync": "2.27.10",
    "browser-sync-webpack-plugin": "2.3.0",
    "concurrently": "7.3.0",
    "copy-webpack-plugin": "11.0.0",
    "core-js": "3.25.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
